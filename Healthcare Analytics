CREATE TABLE BIT_DB.hospitaldata (
index integer PRIMARY KEY,
Health Service Area varchar NOT NULL,
Hospital County varchar NOT NULL,
Operating Certificate Number integer NOT NULL,
Facility ID integer NOT NULL,
Facility Name varchar NOT NULL,
'Age Group' varchar NOT NULL,
Zip Code- 3 digits varchar NOT NULL,
Gender varchar NOT NULL,
Race varchar NOT NULL,
Ethnicity varchar NOT NULL,
'Length of Stay' integer NOT NULL,
'Type of Admission' varchar NOT NULL,
Patient Disposition varchar NOT NULL,
Discharge Year integer NOT NULL,
CCS Diagnosis Code integer NOT NULL,
CCS Diagnosis Description varchar NOT NULL,
CCS Procedure Code NOT NULL 
CCS Procedure Description varchar NOT NULL,
APR DRG Code integer NOT NULL,
APR DRG Description varchar NOT NULL,
APR MDC Code integer NOT NULL,
'APR Severity of Illness Code' integer NOT NULL,
'APR Severity of Illness Description' varchar NOT NULL,
'APR Risk of Mortality' integer NOT NULL,
APR Medical Surgival Description varchar NOT NULL,
'Source of Payment 1' varchar NOT NULL,
'Source of Payment 2' varchar NOT NULL,
'Source of Payment 3' varchar NOT NULL,
Attending Provider License Number varchar NOT NULL,
Operating Provider License Number varchar NOT NULL,
Other Provider License Number varchar NOT NULL,
Birth Weight integer NOT NULL,
Emergency Department Indicator varchar NOT NULL,
Total Charges decimal NOT NULL
Total Costs decimal NOT NULL)
